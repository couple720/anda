<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo1.mapper.AdminMenuMapper">
    <sql id="selectAllFields">
         select id, parent_id, title, path, icon, type, visible, weight, permission, create_time, update_time from t_admin_menu
    </sql>
    <select id="selectById" resultType="com.example.demo1.entity.AdminMenuEntity">
        <include refid="selectAllFields"/>
        where id = #{id}
    </select>
</mapper>
